<!doctype html>

<head>
  <title>Following</title>
  <link rel="stylesheet" type="text/css" href="css/style.css">
</head>

<body>
  <script src="js/nav.js"></script>
  <br><br><br>

  <div class="form-container">
    <h1 class="mobile">Following</h1>
    <div class="post-div">
      <ul>
        <hr>
        <% following.forEach(function(follower) { %>
        <li>
          <div class="profile-name">
            <a href="/search?search=<%=follower.username%>" >
              <p class="mobile-left clean-link"><b><%= follower.username %></b></p>
            </a>
          </div>
        </li>
        <hr>
        <%});%>
      </ul>
    </div>
  </div>
</body>

</html>